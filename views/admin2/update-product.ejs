<body>
  <div class="row">
    <div class="col-sm-2 sidenav bg-danger text-light">
       <h3 class="text-center text-dark">Brand</h3>
       <ul>
       <li class="mt-3 dash text-center text-danger"><a href="/admin-dashboard" class="dash"> Dashboard</a></li>
       <li class="mt-3"><a href="/add-product"><i class="fas fa-shopping-cart mx-2"></i>Add Products</a></li>
       <li class="mt-3"><a href="/productPage"><i class="fas fa-cookie mx-2"></i> Products</a></li>
       <li class="mt-3"><a href=""><i class="fas fa-user mx-2"></i> Customers</a></li>
       <li class="mt-3"><a href=""><i class="fas fa-rocket mx-2"></i> Orders</a></li>
       <li class="mt-3"><a href=""><i class="fas fa-hand-holding-usd mx-2"></i>income</a></li>
       <li class="mt-3"><a href=""><i class="fas fa-question-circle mx-2"></i>Help</a></li>
       <li class="mt-3"><a href=""><i class="fas fa-cogs mx-2"></i>Settings</a></li>
    </ul>
    </div>
 
    <div class="col-sm-10 form ">
    <form action="/updateProd" method="post" enctype="multipart/form-data">
      <h2 class="text-center">Update product!!!!</h2>
      <input type="text" placeholder="Enter product name" name="id" class="form-control rounded-pill" value="<%= prodUp.id %>">
        <div class="mb-3 mt-3">
       
            <input type="text" placeholder="Enter product name" name="name" class="form-control rounded-pill" value="<%= prodUp.name %>">
          </div>
          <div class="mb-3">
   
            <input type="text"  placeholder="Enter Description" name="description" class="form-control rounded-pill" value="<%= prodUp.description %>">
          </div>
          <div class="mb-3">
   
            <input type="number"  placeholder="Enter Price" name="price" class="form-control rounded-pill" value="<%= prodUp.price %>">
          </div>
          <div class="mb-3">
       
            <input type="file"  placeholder="Image Url" name="image" class="form-control rounded-pill" value="<%= prodUp.image %>">
          </div>
          <button type="submit" class="btn btn-danger" style="width: 100%;" id="play">Update Product</button>
    </form>
    </div>
  </div>
  
